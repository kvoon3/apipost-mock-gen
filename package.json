{
  "name": "apipost-mock-gen",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.14.3",
  "description": "_description_",
  "author": "Kevin Kwong <kvoon@duck.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/kvoon3",
  "homepage": "https://github.com/kvoon3/apipost-mock-gen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvoon3/apipost-mock-gen.git"
  },
  "bugs": "https://github.com/kvoon3/apipost-mock-gen/issues",
  "keywords": ["bin"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "cli": "esno src/cli.ts playground/input.md --output playground/output.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "ufo": "^1.5.4",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.10.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.14.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
